{
  "$schema": "https://raw.githubusercontent.com/microsoft/AL-Go/06f80fe0bb7f76e5fa6b321f215eeb68f4ce933f/Actions/settings.schema.json",
  "projectName": "Apps (W1)",
  "appFolders": [
    "../../../src/Apps/W1/*/App",
    "../../../src/Apps/W1/*/*/App"
  ],
  "testFolders": [
    "../../../src/Apps/W1/*/Test",
    "../../../src/Apps/W1/*/Test Library",
    "../../../src/Apps/W1/*/Demo Data",
    "../../../src/Apps/W1/*/*/Test",
    "../../../src/Apps/W1/*/*/Test Library",
    "../../../src/Apps/W1/*/*/Demo Data"
  ],
  "installOnlyReferencedApps": false,
  "ConditionalSettings": [
    {
      "buildModes": [
        "Clean"
      ],
      "settings": {
        "doNotRunTests": true,
        "useCompilerFolder": true,
        "doNotPublishApps": true
      }
    },
    {
      "branches": [
        "releases/*.[0-5]"
      ],
      "settings": {
        "buildModes": [
          "Strict"
        ]
      }
    }
  ],
  "useProjectDependencies": true
}
